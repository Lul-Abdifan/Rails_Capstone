<div id="<%= dom_id recipe_food %>">

  <p>
    <strong>Quantity:</strong>
    <%= recipe_food.quantity %>
  </p>

  <p>
    <strong>Food:</strong>
    <%= recipe_food.food_id %>
  </p>

  <p>
    <strong>Recipe:</strong>
    <%= recipe_food.recipe_id %>
<%= button_to "Destroy this recipe food", recipe_recipe_food_path(recipe_food), method: :delete, data: { confirm: "Are you sure?" } %>

</div>
